{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc && cdk synth",
    "deploy": "cdk deploy --profile digiseed -c environment=prod",
    "deploy-ci": "cdk deploy --profile digiseed --no-execute  --require-approval never -c environment=prod",
    "synth": "cdk synth --profile digiseed -c environment=prod",
    "watch": "tsc -w",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.26.0",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "aws-cdk": "1.26.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/core": "^1.27.0",
    "cdk-spa-deploy": "^0.8.1",
    "source-map-support": "^0.5.16"
  }
}
