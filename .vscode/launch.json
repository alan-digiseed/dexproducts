    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch all tests via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "test"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Launch Start - dexData via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "start - dexdata"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        }

    ]
}